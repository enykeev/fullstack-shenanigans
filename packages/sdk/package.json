    "react": "^16.8 || ^17.0 || ^18.0"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "tsup-preset-solid": "^2.1.0",
    "typescript": "^5.2.2"
  },
  "main": "./dist/index/index.js",
  "types": "./dist/index/index.d.ts",
  "browser": {},
